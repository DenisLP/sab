<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
  <hr></hr>
  <div class="btn-group">
    <%= link_to 'Home', root_path, class: 'btn btn-primary btn-lg ' %>
    <%= link_to 'Contacts', contacts_path, class: 'btn btn-primary btn-lg ' %>
    <!--< %= link_to 'New Contact', new_contact_path, class: 'btn btn-primary btn-lg ' %>-->
    <%= link_to 'Insurances', insurances_path, class: 'btn btn-primary btn-lg ' %>
    <!--< %= link_to 'New Insurance', new_insurance_path, class: 'btn btn-primary btn-lg ' %>-->
    <%= link_to 'Inspections', inspections_path, class: 'btn btn-primary btn-lg ' %>
    <!--< %= link_to 'New Inspection', new_inspection_path, class: 'btn btn-primary btn-lg ' %>-->
    <%= link_to 'Rentals', rentals_path, class: 'btn btn-primary btn-lg ' %>
    <!--< %= link_to 'New Rental', new_rental_path, class: 'btn btn-primary btn-lg ' %>-->
    <%= link_to 'Vehicles', vehicles_path, class: 'btn btn-primary btn-lg ' %>
    <!--< %= link_to 'New Vehicle', new_vehicle_path, class: 'btn btn-primary btn-lg ' %>-->
    <%= link_to 'Chronos', chronos_index_path, class: 'btn btn-primary btn-lg ' %>
  </div>
  <hr></hr>
  <%= yield %>
</div>

</body>
</html>
